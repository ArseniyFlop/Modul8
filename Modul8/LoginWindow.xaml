<Window x:Class="Modul8.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="200" Width="300"
        Icon="iconka.ico">
    <Grid>
        <Label Content="Логин:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0"/>
        <TextBox Name="loginTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="20,45,0,0"/>

        <Label Content="Пароль:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,80,0,0"/>
        <PasswordBox Name="passwordBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="20,105,0,0"/>

        <Button Content="Войти" Name="loginButton" Click="LoginButton_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="20,140,0,0"/>
    </Grid>
</Window>
